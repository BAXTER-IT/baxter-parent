<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/DECORATION/1.1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/DECORATION/1.1.0 http://maven.apache.org/xsd/decoration-1.1.0.xsd"
    name="Baxter Parent">
    
    <bannerLeft>
        <name>${project.name}</name>
        <alt>${project.artifactId}</alt>
    </bannerLeft>
    
    <bannerRight>
        <name>Baxter</name>
        <src>http://www.xcde.net/wp-content/uploads/2012/02/bfx-logo.gif</src>
        <alt>Baxter IT</alt>
        <href>http://baxter-it.com</href>
    </bannerRight>
    
    <publishDate format="yyyy-MM-dd HH:mm" position="left" />
    
    <version position="right" />
    
    <body>
        <menu ref="parent" inherit="bottom"/>
        <menu ref="modules" inherit="bottom"/>
        <menu ref="reports" inherit="bottom" />
    </body>
    
</project>
